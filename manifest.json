{
  "manifest_version": 2,
  "name": "HTML entity selector",
  "description": "Extracts attributes & xpath from the selected HTML entity.",
  "version": "0.1",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": { 
    "default_icon": "icon.png"
  },
  "background":{
    "scripts":["background.js"]
    },
  "web_accessible_resources": [
    "new_object_map/form.html"]
}